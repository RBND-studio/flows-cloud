    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "@types/lodash-es": "^4.17.8",
    "eslint": "8.15.0",
